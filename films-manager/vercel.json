{
    "version": 2,
    "builds": [
      {
        "src": "src/index.js",  
        "use": "@vercel/node"   
      }
    ],
    "routes": [
      {
        "src": "/delivery/routes/(.*)",   
        "dest": "/src/delivery/routes/$1"
      },
      {
        "src": "/(.*)",          
        "dest": "/src/index.js"  
      }
    ]
  }
  