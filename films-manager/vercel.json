{
    "version": 2,
    "builds": [
      {
        "src": "src/index.js",  
        "use": "@vercel/node"   
      }
    ],
    "routes": [
      {
        "src": "/api/films/(.*)",   
        "dest": "/src/delivery/routes/films/$1"
      },
      {
        "src": "/api/users/(.*)",   
        "dest": "/src/delivery/routes/users/$1"
      },
      {
        "src": "/(.*)",          
        "dest": "/src/index.js"  
      }
    ]
  }
  